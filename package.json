{
  "name": "typescript-openapi-gen",
  "version": "0.2.1",
  "description": "Generate TypeScript controllers and routes from OpenAPI specifications with multi-framework support",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "node --loader ts-node/esm --disable-warning=ExperimentalWarning ./bin/dev.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "openapi",
    "typescript",
    "codegen",
    "controller"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.26.2",
  "bin": {
    "tsoapi": "./bin/run.js"
  },
  "oclif": {
    "bin": "tsoapi",
    "commands": "./dist/commands",
    "dirname": "tsoapi",
    "topicSeparator": " "
  },
  "dependencies": {
    "@oclif/core": "^4.8.0",
    "js-yaml": "^4.1.0",
    "openapi-types": "^12.1.3"
  },
  "devDependencies": {
    "@fastify/type-provider-typebox": "^6.1.0",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^18.19.130",
    "elysia": "^1.4.19",
    "express": "^4.18.2",
    "fastify": "^4.28.1",
    "hono": "^4.6.11",
    "markdownlint-cli": "^0.47.0",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16",
    "zod": "^4.0.0"
  }
}
