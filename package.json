{
  "name": "typescript-openapi-gen",
  "version": "1.0.0",
  "description": "Generate TypeScript controller code from OpenAPI specification",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "node --loader ts-node/esm --disable-warning=ExperimentalWarning ./bin/dev.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "openapi",
    "typescript",
    "codegen",
    "controller"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.26.2",
  "bin": {
    "tsoapi": "./bin/run.js"
  },
  "oclif": {
    "bin": "tsoapi",
    "commands": "./dist/commands",
    "dirname": "tsoapi",
    "topicSeparator": " "
  },
  "dependencies": {
    "@oclif/core": "^4.8.0",
    "js-yaml": "^4.1.0",
    "openapi-types": "^12.1.3"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^18.19.130",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  }
}
